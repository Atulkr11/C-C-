import java.util.*;
class Prime
{
public static void main(String k[])
{
Scanner in=new Scanner(System.in);
int n,i,p=0;
System.out.print("Input a whole number:");
n=in.nextInt();
for(i=2;i<=n/2;i++)
{
if(n%i==0)
{
p=1;
break;
}
}
if(p==0)
System.out.println(n+" is a prime number");
else
System.out.println(n+" is not a prime number");
}
}
