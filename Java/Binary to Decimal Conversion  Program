//Binary-To-Decimal
import java.util.*;
class BinaryToDecimal
{
public static void main(String args[])
{
	int bin,dec=0,r,m=1,n;
Scanner in=new Scanner(System.in);
System.out.print("Input binary number:");
	bin=in.nextInt();
while(bin>0)
{
	r=bin%10;
	dec=dec+r*m;
	n=n%2;
	m=m*2;
}
System.out.println("Decimal="+dec);
}
}
